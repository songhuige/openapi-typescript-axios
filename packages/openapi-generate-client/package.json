{
  "name": "openapi-generate-client",
  "version": "1.0.0",
  "author": "liusonghui <itessay@163.com>",
  "license": "MIT",
  "packageManager": "yarn@1.22.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "openapi-gen-cli": "./dist/openapi-gen-cli.cjs"
  },
  "files": [
    "dist",
    "src/template"
  ],
  "scripts": {
    "build": "yarn build-bin && yarn build-index",
    "build-bin": "rollup --config rollup.config.bin.ts --configPlugin typescript && yarn copy-template",
    "build-index": "yarn build-bundle && yarn build-types",
    "build-bundle": "rollup --config rollup.config.ts --configPlugin typescript",
    "build-types-roll": "rollup --config rollup.dts.config.ts --configPlugin typescript",
    "build-types-temp": "tsc --emitDeclarationOnly -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "build-types": "npm run build-types-temp && npm run build-types-roll",
    "copy-template": "ncp src/template dist/template",
    "watch:build-types-temp": "tsc -w --emitDeclarationOnly -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "watch:build-types-roll": "rollup -w --config rollup.dts.config.ts --configPlugin typescript",
    "watch:build-bundle": "yarn build-bundle --watch",
    "watch:build-bin": "yarn build-bin --watch",
    "dev": "yarn build-types-temp && concurrently \"yarn build-bundle --watch\" \"yarn build-bin --watch\" \"yarn watch:build-types-temp\" \"yarn watch:build-types-roll\""
  },
  "devDependencies": {},
  "peerDependencies": {
    "axios": "^1.7.0"
  },
  "dependencies": {
    "cosmiconfig-typescript-loader": "^4.3.0",
    "ts-node": "^10.9.1"
  }
}
