{
  "name": "openapi-parse",
  "version": "1.0.0",
  "author": "liusonghui <itessay@163.com>",
  "license": "MIT",
  "packageManager": "yarn@1.22.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build-bundle && yarn build-types",
    "build-bundle": "rollup --config rollup.config.ts --configPlugin typescript",
    "build-types-roll": "rollup --config rollup.dts.config.ts --configPlugin typescript && rimraf temp",
    "build-types-temp": "tsc --emitDeclarationOnly -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "build-types": "yarn build-types-temp && yarn build-types-roll",
    "dev": "yarn build --watch"
  },
  "devDependencies": {},
  "peerDependencies": {
    "axios": "^1.3.4"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.6.4",
    "ts-node": "^10.9.1"
  }
}
