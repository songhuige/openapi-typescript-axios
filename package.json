{
  "name": "openapi-typescript-gen-monorepo",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "liusonghui <itessay@163.com>",
  "license": "MIT",
  "packageManager": "yarn@1.22.5",
  "type": "module",
  "workspaces": [
    "packages/*",
    "example/*"
  ],
  "scripts": {
    "build-openapi-parse": "yarn workspace openapi-parse build",
    "build-generate-client": "yarn workspace openapi-generate-client build",
    "build": "yarn build-openapi-parse && yarn build-generate-client",
    "example-client": "yarn workspace example-openapi-generate-client generate-client",
    "lint": "eslint **/*.{ts,js,json,md}",
    "lint:fix": "yarn lint --fix",
    "format": "prettier . --write",
    "postinstall": "husky install"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.6.4",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint": "^8.37.0",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.7.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "ncp": "^2.0.0",
    "openapi-typescript": "^6.2.4",
    "prettier": "^2.8.8",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "axios": "^1.7.0"
  },
  "dependencies": {}
}
